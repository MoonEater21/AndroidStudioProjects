<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.user.ActivityMenuUser">
        <androidx.cardview.widget.CardView
                android:layout_width="384dp"
                android:layout_height="434dp" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.384"
                app:layout_constraintVertical_bias="0.377" android:layout_marginStart="1dp">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="414dp">

                <ImageView
                    android:id="@+id/ivListDogsUser"
                    android:layout_width="92dp"
                    android:layout_height="95dp"
                    android:layout_marginStart="28dp"
                    android:layout_marginTop="40dp"
                    app:layout_constraintEnd_toStartOf="@+id/textView9"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:srcCompat="@drawable/listdogsimage" />

                <ImageView
                    android:id="@+id/iv_makananDogUser"
                    android:layout_width="87dp"
                    android:layout_height="79dp"
                    android:layout_marginStart="28dp"
                    android:layout_marginTop="36dp"
                    android:src="@drawable/img"
                    app:layout_constraintEnd_toStartOf="@+id/textView10"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ivMainanDogUser" />

                <ImageView
                    android:id="@+id/ivMainanDogUser"
                    android:layout_width="89dp"
                    android:layout_height="94dp"
                    android:layout_marginStart="28dp"
                    android:layout_marginTop="36dp"
                    android:src="@drawable/imanananjing"
                    app:layout_constraintEnd_toStartOf="@+id/textView11"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/ivListDogsUser" />

                <ImageView
                    android:id="@+id/iv_aboutUser"
                    android:layout_width="43dp"
                    android:layout_height="32dp"
                    android:layout_marginBottom="16dp"
                    android:src="@drawable/about"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.953"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="72dp"
                    android:layout_marginEnd="132dp"
                    android:text="Anjing"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="76dp"
                    android:layout_marginEnd="68dp"
                    android:text="Makanan Anjing"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView11" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="128dp"
                    android:layout_marginEnd="80dp"
                    android:text="Mainan Anjing"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView9" />


            </androidx.constraintlayout.widget.ConstraintLayout>


        </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>